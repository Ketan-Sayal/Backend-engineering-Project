    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-family: Arial, sans-serif;
        }

        :root {
            --primary: #FF9F43;
            --primary-hover: #FF8A25;
            --dark: #2D3436;
            --light-bg: #F8F9FA;
        }

        .hero-section {
            background: linear-gradient(135deg, var(--primary) 0%, var(--primary-hover) 100%);
            padding: 120px 20px 60px;
            text-align: center;
            color: white;
        }

        .hero-section h1 {
            font-size: 3rem;
            margin-bottom: 1.5rem;
        }

        .hero-section p {
            font-size: 1.2rem;
            max-width: 800px;
            margin: 0 auto;
        }

        .prep-container {
            max-width: 1200px;
            margin: 0 auto;
            padding: 40px 20px;
        }

        .resources-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
            gap: 30px;
            margin-top: 40px;
        }

        .resource-card {
            background: white;
            border-radius: 12px;
            overflow: hidden;
            box-shadow: 0 4px 15px rgba(0,0,0,0.1);
            transition: transform 0.3s ease, box-shadow 0.3s ease;
        }

        .resource-card:hover {
            transform: translateY(-10px);
            box-shadow: 0 8px 25px rgba(0,0,0,0.15);
        }

        .resource-image {
            height: 200px;
            width: 100%;
            background-size: cover;
            background-position: center;
            transition: transform 0.3s ease;
        }

        .resource-card:hover .resource-image {
            transform: scale(1.05);
        }

        .resource-content {
            padding: 20px;
        }

        .resource-content h3 {
            color: var(--dark);
            margin-bottom: 10px;
            font-size: 1.4rem;
        }

        .resource-content p {
            color: #666;
            margin-bottom: 15px;
            line-height: 1.5;
        }

        .resource-tags {
            display: flex;
            gap: 10px;
            flex-wrap: wrap;
            margin-bottom: 15px;
        }

        .tag {
            background: var(--light-bg);
            padding: 5px 12px;
            border-radius: 20px;
            font-size: 0.9rem;
            color: var(--dark);
        }

        .btn-explore {
            display: inline-block;
            padding: 10px 20px;
            background: var(--primary);
            color: white;
            text-decoration: none;
            border-radius: 6px;
            transition: background 0.3s ease;
        }

        .btn-explore:hover {
            background: var(--primary-hover);
        }

        .section-title {
            text-align: center;
            margin: 40px 0;
            color: var(--dark);
            font-size: 2.2rem;
        }

        .search-filters {
            display: flex;
            gap: 20px;
            margin-bottom: 40px;
            flex-wrap: wrap;
            justify-content: center;
        }

        .filter-select {
            padding: 12px 24px;
            border: 2px solid #ddd;
            border-radius: 8px;
            font-size: 1rem;
            min-width: 200px;
            cursor: pointer;
        }

        .stats-container {
            display: grid;
            grid-template-columns: repeat(4, 1fr);
            gap: 20px;
            margin: 40px 0;
            text-align: center;
        }

        .stat-card {
            background: var(--light-bg);
            padding: 30px;
            border-radius: 12px;
            transition: transform 0.3s ease;
        }

        .stat-card:hover {
            transform: translateY(-5px);
        }

        .stat-number {
            font-size: 2.5rem;
            color: var(--primary);
            margin-bottom: 10px;
            font-weight: bold;
        }

        .stat-label {
            color: var(--dark);
            font-size: 1.1rem;
        }

        @media (max-width: 768px) {
            .stats-container {
                grid-template-columns: repeat(2, 1fr);
            }
            
            .hero-section h1 {
                font-size: 2.2rem;
            }
        }

        @media (max-width: 480px) {
            .stats-container {
                grid-template-columns: 1fr;
            }
        }
