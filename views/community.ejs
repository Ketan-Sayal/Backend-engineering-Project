<!-- community page  -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>University Community Page</title>
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: 'Arial', sans-serif;
            background-color: #f8f9fa;
            color: #333;
            line-height: 1.6;
        }

        header {
            background-color: #0056b3;
            color: #fff;
            padding: 20px;
            text-align: center;
        }

        header h1 {
            font-size: 36px;
        }

        .container {
            max-width: 1200px;
            margin: 20px auto;
            padding: 20px;
        }

        .intro {
            text-align: center;
            margin-bottom: 40px;
        }

        .intro h2 {
            font-size: 28px;
            margin-bottom: 10px;
        }

        .intro p {
            font-size: 16px;
            color: gray;
        }

        .cards {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 20px;
        }

        .card {
            background: #fff;
            border: 1px solid #ddd;
            border-radius: 10px;
            overflow: hidden;
            transition: transform 0.3s, box-shadow 0.3s;
        }

        .card:hover {
            transform: translateY(-10px);
            box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);
        }

        .card img {
            width: 100%;
            height: 200px;
            object-fit: cover;
        }

        .card-content {
            padding: 20px;
        }

        .card-content h3 {
            font-size: 20px;
            color: #0056b3;
            margin-bottom: 10px;
        }

        .card-content p {
            font-size: 14px;
            color: #666;
            margin-bottom: 15px;
        }

        .card-content a {
            display: inline-block;
            text-decoration: none;
            color: #fff;
            background: #0056b3;
            padding: 10px 20px;
            border-radius: 5px;
            font-size: 14px;
            transition: background-color 0.3s;
        }

        .card-content a:hover {
            background-color: #004494;
        }

        .community-section {
            margin-top: 40px;
        }

        .community-section h2 {
            font-size: 28px;
            color: #333;
            margin-bottom: 20px;
        }

        .community-section ul {
            list-style: none;
        }

        .community-section ul li {
            font-size: 16px;
            color: #555;
            margin: 10px 0;
            display: flex;
            align-items: center;
        }

        .community-section ul li i {
            color: #0056b3;
            margin-right: 10px;
        }

        footer {
            background: #0056b3;
            color: #fff;
            text-align: center;
            padding: 20px;
            margin-top: 40px;
        }

        footer p {
            font-size: 14px;
        }
        .main-nav {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 10px 20px;
    background-color: #111;
    color: #fff;
}

.nav-brand {
    display: flex;
    align-items: center;
    gap: 8px;
}

.nav-logo {
    height: 30px;
}

.brand-text {
    font-size: 18px;
    font-weight: bold;
    color: #f4a100;
}

.nav-menu {
    list-style: none;
    display: flex;
    gap: 20px;
    margin: 0;
    padding: 0;
}

.nav-menu a {
    text-decoration: none;
    color: #fff;
    font-size: 16px;
    font-weight: 500;
    transition: color 0.3s;
}

.nav-menu a:hover {
    color: #f4a100;
}

.nav-buttons {
    display: flex;
    align-items: center;
    gap: 10px;
}

.btn-fee {
    text-decoration: none;
    padding: 8px 15px;
    border-radius: 5px;
    font-size: 14px;
    font-weight: bold;
    text-align: center;
    background-color: #fff;
    color: #111;
    border: 1px solid #fff;
    transition: all 0.3s;
}

.btn-fee:hover {
    background-color: #f4a100;
    color: #fff;
    border-color: #f4a100;
}

.btn-login {
    text-decoration: none;
    padding: 8px 15px;
    border-radius: 5px;
    font-size: 14px;
    font-weight: bold;
    text-align: center;
    color: #fff;
    background: none;
    transition: color 0.3s;
}

.btn-login:hover {
    color: #f4a100;
}

.btn-signup {
    text-decoration: none;
    padding: 8px 15px;
    border-radius: 5px;
    font-size: 14px;
    font-weight: bold;
    text-align: center;
    background-color: #28a745;
    color: #fff;
    transition: background-color 0.3s;
}

.btn-signup:hover {
    background-color: #218838;
}
.nav-group a:hover {
    color: #ffc107;
}
.custom-footer {
        background: #1a1a1a;
        color: white;
        padding: 40px 20px;
        font-family: Arial, sans-serif;
    }

    .custom-footer-container {
        max-width: 1200px;
        margin: 0 auto;
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
        gap: 30px;
    }

    .custom-footer .logo {
        height: 40px;
        margin-bottom: 20px;
    }

    .custom-nav-group h3 {
        color: white;
        margin-bottom: 15px;
        font-size: 16px;
    }

    .custom-nav-group ul {
        list-style: none;
        padding: 0;
    }

    .custom-nav-group li {
        margin-bottom: 10px;
    }

    .custom-nav-group a {
        color: #ffffff;
        text-decoration: none;
        font-size: 14px;
    }

    .custom-nav-group a:hover {
        color: #ffc107;
    }

    .custom-social-icons {
        display: flex;
        gap: 15px;
        margin-top: 20px;
    }

    .custom-social-icon {
        background: #ffc107;
        width: 35px;
        height: 35px;
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        color: black;
        text-decoration: none;
    }

    .custom-newsletter {
        display: flex;
        gap: 10px;
        margin-top: 15px;
    }

    .custom-newsletter input {
        padding: 10px;
        border: none;
        flex-grow: 1;
        border-radius: 4px;
    }

    .custom-newsletter button {
        background: #ffc107;
        border: none;
        padding: 10px 20px;
        cursor: pointer;
        border-radius: 4px;
    }

    .custom-copyright {
        text-align: center;
        margin-top: 40px;
        padding-top: 20px;
        border-top: 1px solid #333;
        font-size: 12px;
        color: #888;
    }
    </style>
</head>
<body>
    <navbar>
        <!-- Replace this nav section -->
        <nav class="main-nav">
            <div class="nav-brand">
                <span 
                    class="brand-text" 
                    onclick="navigateTo('/home')" 
                    style="text-decoration: none; cursor: pointer;"
                >
                    TOPUNIVERSITIES
                </span>
            </div>
            
            <ul class="nav-menu">
                <li style="cursor: pointer;" onclick="navigateTo('/rankings')">RANKINGS</li>
                <li style="cursor: pointer;" onclick="navigateTo('/discover')">DISCOVER</li>
                <li style="cursor: pointer;" onclick="navigateTo('/events')">EVENTS</li>
                <li style="cursor: pointer;" onclick="navigateTo('/prepare')">PREPARE</li>
                <li style="cursor: pointer;" onclick="navigateTo('/careers')">CAREERS</li>
                <li style="cursor: pointer;" onclick="navigateTo('/community')">COMMUNITY</li>
                <li style="cursor: pointer;" onclick="navigateTo('/courses')">COURSES</li>
                <li style="cursor: pointer;" onclick="navigateTo('/Resources')">RESOURCES</li>
            </ul>
            
            <div class="nav-buttons">
                <button onclick="navigateTo('/feestructure')" class="btn-fee" style="cursor: pointer;">Fee Structure</button>
                <button onclick="navigateTo('/feestructure')" class="btn-fee" style="cursor: pointer;"><%=name%></button>
            </div>
        </nav>
        
    </navbar>
    <header>
        <h1>University Community</h1>
        <p>Connect. Collaborate. Celebrate.</p>
    </header>

    <div class="container">
        <section class="intro">
            <h2>Welcome to Our Community</h2>
            <p>Join our vibrant community of students, faculty, and alumni. Stay updated, share ideas, and participate in events.</p>
        </section>

        <section class="cards">
            <div class="card">
                <img src="https://img.freepik.com/free-vector/coming-soon-announcement-background-with-megaphone_1017-39308.jpg?t=st=1735121372~exp=1735124972~hmac=cd07ca0d60ada6b04ed1543292f832e3101f7e7ce6aca174ef61dbcfa7b8a0ef&w=1480" alt="Announcements">
                <div class="card-content">
                    <h3>Latest Announcements</h3>
                    <p>Stay informed with the latest news and updates from the university.</p>
                    <a href="#">View Announcements</a>
                </div>
            </div>
            <div class="card">
                <img src="https://images.pexels.com/photos/4940642/pexels-photo-4940642.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=2" alt="Events">
                <div class="card-content">
                    <h3>Upcoming Events</h3>
                    <p>Don't miss out on exciting events, workshops, and seminars.</p>
                    <a href="#">View Events</a>
                </div>
            </div>
            <div class="card">
                <img src="https://images.pexels.com/photos/8439697/pexels-photo-8439697.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=2" alt="Forums">
                <div class="card-content">
                    <h3>Discussion Forums</h3>
                    <p>Connect with peers, ask questions, and share your ideas.</p>
                    <a href="#">Join the Discussion</a>
                </div>
            </div>
        </section>

        <section class="community-section">
            <h2>Get Involved</h2>
            <ul>
                <li><i>&#9733;</i> Start a club or join one that interests you.</li>
                <li><i>&#9733;</i> Participate in university-wide events and competitions.</li>
                <li><i>&#9733;</i> Share your success stories to inspire others.</li>
                <li><i>&#9733;</i> Volunteer for community service projects.</li>
            </ul>
        </section>
    </div>
    <footer class="custom-footer">
        <div class="custom-footer-container">
            <div class="custom-nav-group">
                <img src="/api/placeholder/150/40" alt="QS Top Universities" class="logo">
                <div class="custom-social-icons">
                    <a href="#" class="custom-social-icon">f</a>
                    <a href="#" class="custom-social-icon">𝕏</a>
                    <a href="#" class="custom-social-icon">in</a>
                    <a href="#" class="custom-social-icon">▶</a>
                    <a href="#" class="custom-social-icon">📷</a>
                </div>
            </div>

            <div class="custom-nav-group">
                <h3>For Students</h3>
                <ul>
                    <li><a href="#">Rankings</a></li>
                    <li><a href="#">Find a Course</a></li>
                    <li><a href="#">Student Guide</a></li>
                </ul>
            </div>

            <div class="custom-nav-group">
                <h3>For Institution</h3>
                <ul>
                    <li><a href="#">Partner with Us</a></li>
                    <li><a href="#">Analytics</a></li>
                    <li><a href="#">Resources</a></li>
                </ul>
            </div>

            <div class="custom-nav-group">
                <h3>Legal</h3>
                <ul>
                    <li><a href="#">Privacy</a></li>
                    <li><a href="#">Terms & Conditions</a></li>
                    <li><a href="#">Data Copyright</a></li>
                    <li><a href="#">Cookies</a></li>
                </ul>
            </div>

            <div class="custom-nav-group">
                <h3>Subscribe to Our Newsletter</h3>
                <div class="custom-newsletter">
                    <input type="email" placeholder="Please enter your email">
                    <button>→</button>
                </div>
            </div>
        </div>

        <div class="custom-copyright">
            © QS Quacquarelli Symonds Limited 1994 - 2024. All rights reserved.
        </div>
    </footer>
